        <activity android:name=".ui.info.DataShareActivity" />
        <activity android:name=".ui.info.CallSettingActivity">
        </activity>
        <activity android:name=".ui.info.RemindDetailActivity" />
        <activity android:name=".ui.info.DeviceRemindActivity" />
        <activity android:name=".ui.info.UrineDetailActivity" />
        <activity android:name=".ui.info.PickSizeActivity" />
        <activity android:name=".ui.info.ChangeInfoActivity" />
        <activity android:name=".ui.user.ForgetActivity" />
        <activity android:name=".ui.user.RegisterActivity" />
        <activity android:name=".ui.user.LoginNormalActivity" />
        <activity android:name=".ui.user.LoginThirdActivity" />
        <activity android:name=".ui.user.LoginActivity" />
        <activity android:name=".ui.main.MainActivity" />
    </application>

</manifest>